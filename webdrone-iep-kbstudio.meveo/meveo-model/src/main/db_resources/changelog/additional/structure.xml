    <changeSet id="service_endpoint" author="ClÃ©ment Bareth">
        <!-- Service endpoint -->
        <createTable tableName="service_endpoint">
            <column name="id" type="bigint" autoIncrement="true">
                <constraints primaryKey="true"/>
            </column>
